{
  "name": "cerpus/re-content-client",
  "description": "A Laravel client to make working with the Recommendation Engine Content Index a breeze",
  "type": "library",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Odd-Arne Johansen",
      "email": "odd-arne.johansen@cerpus.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "^6.0",
    "illuminate/support": "^6.0|^7.0",
    "cerpus/cerpushelper": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Cerpus\\REContentClient\\": "src\/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cerpus\\REContentClient\\Test\\": "tests\/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cerpus\\REContentClient\\REContentClientProvider"
      ]
    }
  }
}
